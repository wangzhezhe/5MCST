FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    top
    {
        type           symmetry;
    }

    bottom
    {
	type           symmetry;

    }

    wall
    {
	type           zeroGradient;
    }

    inlet
    {
        type           zeroGradient;
    }

    outlet
    {
        type           fixedValue;
	value          uniform 0.5;
    }

    frontandback
    {
        type            empty;
    }
}


