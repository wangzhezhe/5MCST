FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    top
    {
        type           zeroGradient;
    }

    bottom
    {
	type           zeroGradient;

    }

    cylinderwall
    {
	type           zeroGradient;
    }

    left
    {
        type           zeroGradient;
    }

    right
    {
        type           fixedValue;
	value          uniform 1;
    }

    frontandback
    {
        type            empty;
    }
}


